                            <image name="pic" alias="PIC" width="75" height="80" xpos="18" ypos="55" background="#ffffff" source="images/profile.png" ignoretouches="yes"/>  
                            <image name="pic-complete" alias="PIC-COMPLETE" width="75" height="80" xpos="18" ypos="55" background="#ffffff" asset=" " ignoretouches="yes" alpha="0"/>  
                            <text name="edit" alias="EDIT" width="100" height="20" xpos="6" ypos="132" font="Helvetica" color="#ffffff" size="10" alignment="center" text="Edit Image" ignoretouches="yes"/> 
                          </panel>
                        
